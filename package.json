{
  "name": "rockhold.dev",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "rm -rf node_modules/.vite .vite dist",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "e2e": "cypress open",
    "e2e:headless": "cypress run --browser chrome",
    "ct": "cypress open --component",
    "ct:headless": "cypress run --component --browser chrome",
    "test:all": "npm run test && npm run ct:headless && npm run e2e:headless",
    "test:ci": "npm run lint && npm run test:coverage && npm run ct:headless && npm run e2e:headless",
    "check": "npm run lint && npm run test",
    "check:all": "npm run lint && npm run test:all"
  },
  "dependencies": {
    "@vercel/analytics": "1.5.0",
    "@vercel/speed-insights": "1.2.0",
    "clsx": "2.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-icons": "5.5.0",
    "react-router-dom": "7.8.2"
  },
  "devDependencies": {
    "@cypress/react": "9.0.1",
    "@eslint/js": "9.37.0",
    "@tailwindcss/vite": "4.1.12",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@types/jest": "30.0.0",
    "@types/react": "19.1.10",
    "@types/react-dom": "19.1.9",
    "@types/react-router-dom": "5.3.3",
    "@types/testing-library__jest-dom": "5.14.9",
    "@vitejs/plugin-react": "5.0.0",
    "@vitest/coverage-v8": "3.2.4",
    "autoprefixer": "10.4.21",
    "cypress": "15.0.0",
    "eslint": "9.37.0",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-react-refresh": "0.4.20",
    "globals": "16.3.0",
    "jsdom": "27.0.0",
    "tailwindcss": "4.1.12",
    "typescript": "5.8.3",
    "typescript-eslint": "8.39.1",
    "vite": "7.1.9",
    "vitest": "3.2.4"
  }
}
